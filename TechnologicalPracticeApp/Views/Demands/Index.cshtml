@model IEnumerable<TechnologicalPracticeApp.Models.Demand>

@{
    ViewBag.Title = "Index";
}

<h2>Правила экомаркировки</h2>

<p>
    @Html.ActionLink("Добавить новое", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Правило
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Rule)
        </td>
        <td>
            @Html.ActionLink("Редактировать", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
